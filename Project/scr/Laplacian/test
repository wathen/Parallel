echo $"\n\n\nmpirun -np 1 python Laplacian.py 1"
mpirun -np 1 python Laplacian.py 1 >> results_N=1 2>&1
echo $"\n\n\nmpirun -np 2 python Laplacian.py 1"
mpirun -np 2 python Laplacian.py 1 &>> results_N=1 
echo $"\n\n\nmpirun -np 4 python Laplacian.py 1"
mpirun -np 4 python Laplacian.py 1 &>> results_N=1 
echo $"\n\n\nmpirun -np 8 python Laplacian.py 1"
mpirun -np 8 python Laplacian.py 1 &>> results_N=1 

